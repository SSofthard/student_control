<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
<!--
    Áreas de conocimiento de las carreras ofertadas en la UNEFA Núcleo caracas
-->
    <record id="ar_cto_ing_arq_tec" model="unefa.area_conocimiento">
        <field name="area_conocimiento">INGENIERÍA, ARQUITECTURA Y TECNOLOGÍA</field>
    </record>
    
    <record id="ar_cto_cnc_agr_mar" model="unefa.area_conocimiento">
        <field name="area_conocimiento">CIENCIAS DEL AGRO Y EL MAR</field>
    </record>
    
    <record id="ar_cto_cnc_salud" model="unefa.area_conocimiento">
        <field name="area_conocimiento">CIENCIAS DE LA SALUD</field>
    </record>
    
    <record id="ar_cto_cnc_educacion" model="unefa.area_conocimiento">
        <field name="area_conocimiento">CIENCIAS DE LA EDUCACIÓN</field>
    </record>
    
    <record id="ar_cto_cnc_sociales" model="unefa.area_conocimiento">
        <field name="area_conocimiento">CIENCIAS SOCIALES</field>
    </record>
    

    <record id="carrera_ING_SIS" model="unefa.carrera">
        <field name="nombre">INGENIERÍA DE SISTEMAS</field>
        <field name="codigo">ING-SIS</field>
        <field name="area_conocimiento_id" ref="ar_cto_ing_arq_tec"/>
        <field name="tipo">larga</field>
        <field name="state">activo</field>
        <field name="nucleo_ids" eval="[(6, 0, [ref('unefa_nucleo_caracas')])]" />
    </record>


    </data>
</openerp>

